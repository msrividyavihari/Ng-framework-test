{
    "name": "liftapp",
    "description": "LIFT App",
    "version": "1.0.0",
    "private": true,
    "installConfig": {
        "pnp": true
    },
    "prettier": "@d-lift/scripts/prettier-config",
    "dependencies": {
        "@d-lift/core": ">=4.0.0",
        "@d-lift/scripts": ">=4.0.0",
        "@d-lift/theme-nextgen360": ">=4.0.0",
        "@d-lift/uxcomponents": ">=4.0.0",
        "md5": "^2.3.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "sugar": "^2.0.6"
    },
    "scripts": {
        "start": "port=5000 yarn lift-start dev",
        "build": "yarn lift-start build",
        "test": "yarn lift-start test",
        "test-cov": "yarn lift-start test-cov",
        "analyze": "yarn lift-start analyze",
        "pretty": "yarn prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md,rt}\"",
        "pre-commit": "yarn pretty",
        "pre-push": "yarn pretty",
        "upgrade": "yarn up @d-lift/core @d-lift/cli @d-lift/uxcomponents @d-lift/scripts @d-lift/theme-nextgen360"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "ie 11"
    ],
    "repository": "http://elb-bitbucket-746777543.us-east-2.elb.amazonaws.com/scm/ux/uxtoolkit.git",
    "author": "Deloitte Consulting LLC",
    "license": "SEE LICENSE IN LICENSE.txt",
    "devDependencies": {
        "@d-lift/cli": ">=4.0.0",
        "@testing-library/dom": "^5.0.0",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
        "babel-polyfill": "^6.26.0",
        "enzyme": "^3.11.0",
        "eslint-config-react-app": "^5.2.0",
        "husky": "^4.2.3",
        "lodash": "^4.17.11",
        "prettier": "^2.3.2",
        "react-app-polyfill": "^1.0.6"
    }
}
